<?xml version="1.0" ?>

<!--

-->

<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" "http://castor.exolab.org/mapping.dtd">
<mapping>

    <class name="org.gridlab.gridsphere.services.resource.ResourceTypeRegistry">
        <map-to xml="resource-types"/>
        <field name="resourcetypes" collection="arraylist" type="org.gridlab.gridsphere.services.resource.ResourceTypeDescription">
            <bind-xml name="resource-type"/>
        </field>
    </class>

    <class name="org.gridlab.gridsphere.services.resource.ResourceTypeDescription">
        <field name="Name" type="string">
            <bind-xml name="name"/>
        </field>
        <field name="Id" type="string">
            <bind-xml name="id"/>
        </field>
        <field name="Type" type="string">
            <bind-xml name="type"/>
        </field>
        <field name="Classname" type="string">
            <bind-xml name="classname"/>
        </field>
        <field name="attributes" collection="arraylist" type="java.lang.String">
            <bind-xml name="attribute"/>
        </field>

    </class>

</mapping>