<?xml version="1.0" ?>

<!--

-->

<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" "http://castor.exolab.org/mapping.dtd">
<mapping>

    <class name="org.gridlab.gridsphere.services.ui.job.JobProfileRegistry">
        <map-to xml="job-profiles"/>
        <field name="jobProfileList" collection="arraylist"
               type="org.gridlab.gridsphere.services.ui.job.generic.GenericJobProfile">
            <bind-xml name="job-profile"/>
        </field>
    </class>

    <class name="org.gridlab.gridsphere.services.ui.job.generic.GenericJobProfile">
        <field name="Order" type="int">
            <bind-xml name="order" node="attribute"/>
        </field>
        <field name="Key" type="string">
            <bind-xml name="key" node="attribute"/>
        </field>
        <field name="Description" type="string">
            <bind-xml name="description" node="attribute"/>
        </field>
        <field name="JobViewPageClassName" type="string">
            <bind-xml name="job-view-page"/>
        </field>
        <field name="SubmittedJobViewPageClassName" type="string">
            <bind-xml name="submitted-job-view-page"/>
        </field>
        <field name="CanceledJobViewPageClassName" type="string">
            <bind-xml name="canceled-job-view-page"/>
        </field>
        <field name="DeletedJobViewPageClassName" type="string">
            <bind-xml name="deleted-job-view-page"/>
        </field>
        <field name="JobSpecViewPageClassName" type="string">
            <bind-xml name="job-spec-view-page"/>
        </field>
        <field name="JobSpecEditPageClassNameList" collection="arraylist" type="string">
            <bind-xml name="job-spec-edit-page"/>
        </field>
        <field name="MigratedJobViewPageClassName" type="string">
            <bind-xml name="migrated-job-view-page" node="attribute"/>
        </field>
    </class>

</mapping>
