    <portlet>
        <description xml:lang="en">Shows RSS News</description>
        <description xml:lang="de">Zeigt RSS Seiten an.</description>
        <portlet-name>RssReaderPortlet</portlet-name>
        <display-name xml:lang="en">RSS Reader</display-name>
        <display-name xml:lang="de">RSS Reader</display-name>
        <portlet-class>org.gridlab.gridsphere.extras.portlets.rssreader.RssReaderPortlet</portlet-class>
        <expiration-cache>60</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>config</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <supports>
            <mime-type>text/wml</mime-type>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>RSS Reader</title>
            <short-title>RssReader</short-title>
            <keywords>Rss, Reader</keywords>
        </portlet-info>
        <security-role-ref>
            <role-name>GUEST</role-name>
            <role-link>auth-user</role-link>
        </security-role-ref>
    </portlet>

