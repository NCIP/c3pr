<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.gridlab.gridsphere.extras.services.wiki.Snip"
        table="snip"
        >

        <id
            name="oid"
            column="gsoid"
            type="java.lang.String"
            length="32"
            >
            <generator class="uuid.hex"/>

        </id>

        <property name="Userid" type="string" column="snip_userid"/>
        <property name="name" type="string" column="snip_name"/>
        <property name="content" type="java.lang.String" length="65000" column="snip_content"/>
    </class>


</hibernate-mapping>
