<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="edu.duke.cabig.c3pr.domain">
    <class name="Study" table="STUDYS" lazy="false">
        <id name="id" column="ID" type="java.lang.Integer">
            <generator class="native">
                <param name="sequence">C3PR_GENERAL_SEQ</param>
            </generator>
        </id>
        <property name="blindedIndicator" column="BLINDED_INDICATOR" type="java.lang.String" />
        <property name="descriptionText" column="DESCRIPTION_TEXT" type="java.lang.String" />
        <property name="diseaseCode" column="DISEASE_CODE" type="java.lang.String" />
        <property name="longTitleText" column="DISEASE_CODE_LONG_TITLE_TEXT" type="java.lang.String" />
        <property name="monitorCode" column="MONITOR_CODE" type="java.lang.String" />
        <property name="multiInstitutionIndicator" column="MULTI_INSTITUTION_INDICATOR" type="java.lang.String" />
        <property name="nciIdentifier" column="NCI_IDENTIFIER" type="java.lang.String" />
        <property name="phaseCode" column="PHASE_CODE" type="java.lang.String" />
        <property name="precisText" column="PRECIS_TEXT" type="java.lang.String" />
        <property name="randomizedIndicator" column="RANDOMIZED_INDICATOR" type="java.lang.String" />
        <property name="shortTitleText" column="SHORT_TITLE_TEXT" type="java.lang.String" />
        <property name="sponsorCode" column="SPONSOR_CODE" type="java.lang.String" />
        <property name="status" column="STATUS_CODE" type="java.lang.String" />
        <property name="targetAccrualNumber" column="TARGET_ACCRUAL_NUMBER" type="java.lang.Integer" />
        <property name="type" column="TYPE_CODE" type="java.lang.String" />
    </class>
</hibernate-mapping>
