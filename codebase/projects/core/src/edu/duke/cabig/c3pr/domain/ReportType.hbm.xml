<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="edu.duke.cabig.c3pr.domain">

	<class name="ReportType" table="CODE_TYPE" mutable="false" lazy="false">
		<id name="id" type="java.lang.String" column="TYPE_CODE"></id>
		<property name="description" column="TYPE_DESC" type="java.lang.String" />
	</class>
	
</hibernate-mapping>
