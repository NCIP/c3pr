<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<mapping>
	<class name="edu.duke.cabig.c3pr.domain.Accrual.SiteAccrualReport">
		<map-to xml="siteAccrualReport" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="ctepId" type="string">
			<bind-xml name="ctepId" node="element" />
		</field>
		<field name="address" type="string">
			<bind-xml name="address" node="element" />
		</field>
		<field name="lat" type="Double">
			<bind-xml name="lat" node="element" />
		</field>
		<field name="lng" type="Double">
			<bind-xml name="lng" node="element" />
		</field>
		<field name="accrual" type="edu.duke.cabig.c3pr.domain.accrual.Accrual">
			<bind-xml name="accrual" node="element" />
		</field>
		<field name="studyAccrualReports"
			type="edu.duke.cabig.c3pr.domain.accrual.StudyAccrualReport"
			collection="collection" get-method="getStudyAccrualReports"
			set-method="setStudyAccrualReports">
			<bind-xml node="element" auto-naming="deriveByClass"/>
		</field>
	</class>
	<class name="edu.duke.cabig.c3pr.domain.accrual.StudyAccrualReport">
		<map-to xml="organization" />
		<field name="shortTitle" type="string">
			<bind-xml name="shortTitle" node="element" />
		</field>
		<field name="identifier" type="string">
			<bind-xml name="identifier" node="element" />
		</field>
		<field name="diseaseSiteAccrualReports"
			type="edu.duke.cabig.c3pr.domain.accrual.DiseaseSiteAccrualReport"
			collection="collection" get-method="getDiseaseSiteAccrualReports"
			set-method="setDiseaseSiteAccrualReports">
			<bind-xml node="element" auto-naming="deriveByClass"/>
		</field>
		<field name="accrual" type="edu.duke.cabig.c3pr.domain.accrual.Accrual">
			<bind-xml name="accrual" node="element" />
		</field>
	</class>
	<class name="edu.duke.cabig.c3pr.domain.accrual.Accrual">
		<map-to xml="accrual" />
		<field name="value" type="string">
			<bind-xml name="value" node="attribute" />
		</field>
	</class>
	<class name="edu.duke.cabig.c3pr.domain.accrual.DiseaseSiteAccrualReport">
		<map-to xml="diseaseSiteAccrualReport" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="accrual" type="edu.duke.cabig.c3pr.domain.accrual.Accrual">
			<bind-xml name="accrual" node="element" />
		</field>
	</class>
</mapping>