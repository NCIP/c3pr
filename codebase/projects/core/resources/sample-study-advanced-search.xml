<?xml version="1.0" encoding="iso-8859-1"?>
<queryBuilder xmlns="http://semanticbits.com/querybuilder">
	<target-object>
		<class-name>edu.duke.cabig.c3pr.domain.Study</class-name>
		<display-name>Study</display-name>
		<table-alias>Study_xx</table-alias>
		<dependent-object>
			<class-name>edu.duke.cabig.c3pr.domain.Study</class-name>
			<display-name>Study</display-name>
			<table-alias>Study_xx</table-alias>
			<search-criteria>
				<name>shortTitleText</name>
				<label>Short Title</label>
				<data-type>String</data-type>
			</search-criteria>
			<search-criteria>
				<name>longTitleText</name>
				<label>Long Title</label>
				<data-type>String</data-type>
			</search-criteria>
			<search-criteria>
				<name>phaseCode</name>
				<label>Phase</label>
				<data-type>String</data-type>
			</search-criteria>
			<search-criteria>
				<name>coordinatingCenterStudyStatus.code</name>
				<label>Gender</label>
				<data-type>CoordinatingCenterStudyStatus</data-type>
			</search-criteria>
			<search-criteria>
				<name>type</name>
				<label>Type</label>
				<data-type>String</data-type>
			</search-criteria>
			<search-criteria>
				<name>therapeuticIntentIndicator</name>
				<label>Therapeutic Intent Indicator</label>
				<data-type>Boolean</data-type>
			</search-criteria>
			<search-criteria>
				<name>companionIndicator</name>
				<label>Companion Indicator</label>
				<data-type>Boolean</data-type>
			</search-criteria>
		</dependent-object>
		<!-- Adding study version as dependent object to search study on study version criterion-->
		<dependent-object>
			<class-name>edu.duke.cabig.c3pr.domain.StudyVersion</class-name>
			<display-name>Study Version</display-name>
			<table-alias>studyVersion_xx</table-alias>
			<search-criteria>
				<name>versionStatus.code</name>
				<label>Amendment Status</label>
				<data-type>StatusType</data-type>
			</search-criteria>
			<search-criteria>
				<name>openDate</name>
				<label>Open Date</label>
				<data-type>Date</data-type>
			</search-criteria>
			<association>
				<name>edu.duke.cabig.c3pr.domain.StudyVersion</name>
				<role-name>studyVersion</role-name>
				<table-alias>StudyVersion_xx</table-alias>
				<joining-condition>StudyVersion_xx in Study_xx.studyVersion</joining-condition>
			</association>
		</dependent-object>
		<!-- Adding identifier , because user can search study by identifier value parameters-->
		<dependent-object>
			<class-name>edu.duke.cabig.c3pr.domain.Identifier</class-name>
			<display-name>Identifier</display-name>
			<table-alias>Identifier_xx</table-alias>
			<search-criteria>
				<name>value</name>
				<label>Identifier</label>
				<data-type>String</data-type>
			</search-criteria>
			<association>
				<name>edu.duke.cabig.c3pr.domain.Identifier</name>
				<role-name>identifiers</role-name>
				<table-name>identifiers</table-name>
				<table-alias>Identifier_xx</table-alias>
				<joining-condition>Identifier_xx = any elements(Study_xx.identifiers)</joining-condition>
			</association>
		</dependent-object>
		<!-- adding study org as dependent object so that use can search study by coordinating center, funding sponsor and study site -->
		<dependent-object>
			<class-name>edu.duke.cabig.c3pr.domain.StudyOrganization</class-name>
			<display-name>Study Organization</display-name>
			<table-alias>StudyOrganization_xx</table-alias>
			<search-criteria>
				<name>id</name>
				<label>Id</label>
				<data-type>Integer</data-type>
			</search-criteria>
			<search-criteria>
				<name>dType</name>
				<label>Type</label>
				<data-type>String</data-type>
			</search-criteria>
			<association>
				<name>edu.duke.cabig.c3pr.domain.StudyOrganization</name>
				<table-alias>StudyOrganization_xx</table-alias>
				<joining-condition>Study_xx.studyOrganizations = StudyOrganization_xx</joining-condition>
			</association>
		</dependent-object>
		<!-- Adding Study Investigator , because user can search study  by study investigator-->
		<dependent-object>
			<class-name>edu.duke.cabig.c3pr.domain.StudyInvestigator</class-name>
			<display-name>Study Investigator</display-name>
			<table-alias>studyInvestigator_xx</table-alias>
			<search-criteria>
				<name>id</name>
				<label>Id</label>
				<data-type>Integer</data-type>
			</search-criteria>
			<search-criteria>
				<name>roldeCode</name>
				<label>Role</label>
				<data-type>String</data-type>
			</search-criteria>
			<association>
				<name>edu.duke.cabig.c3pr.domain.StudyOrganization</name>
				<table-alias>studyOrganization_xx</table-alias>
				<joining-condition>Study_xx.studyOrganizations = studyOrganization_xx</joining-condition>
			</association>
			<association>
				<name>edu.duke.cabig.c3pr.domain.StudyInvestigator</name>
				<table-alias>studyInvestigator_xx</table-alias>
				<joining-condition>Study_xx.identifiers = Identifier_xx</joining-condition>
			</association>
		</dependent-object>
	</target-object>
</queryBuilder>