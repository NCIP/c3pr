<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://semanticbits.com/registration.xsd" xmlns="http://semanticbits.com/registration.xsd" elementFormDefault="qualified"> <xs:annotation>  <xs:documentation xml:lang="en">   Registartion schema, referring c3prV2-I2.EAP  </xs:documentation> </xs:annotation> <xs:element name="registration" type="RegistrationType"/>  <xs:complexType name="RegistrationType">  <xs:sequence>   <xs:element name="informedConsentFormSignedDate" type="xs:date"/>   <xs:element name="enrollmentDate" type="xs:date"/>   <xs:element name="studyParticipantIdentifier" type="xs:string"/>   <xs:element name="eligibilityIndicator" type="xs:boolean"/>   <xs:element name="participant"  type="ParticipantType"/>   <xs:element name="study"  type="StudyType"/>   <xs:element name="identifier"  type="IdentifierType" minOccurs="1" maxOccurs="unbounded"/>  </xs:sequence>  <xs:attribute name="healthCareSiteGridId" type="xs:string"/>  <xs:attribute name="studyGridId" type="xs:string"/> </xs:complexType> <xs:complexType name="ParticipantType">  <xs:sequence>   <xs:element name="administrativeGenderCode"   type="xs:string"/>   <xs:element name="birthDate" type="xs:date"/>   <xs:element name="ethnicGroupCode"   type="xs:string"/>   <xs:element name="firstName"  type="xs:string"/>   <xs:element name="lastName"  type="xs:string"/>   <xs:element name="maritalStatusCode"  type="xs:string"/>   <xs:element name="raceCode"  type="xs:string"/>   <xs:element name="identifier"  type="IdentifierType" minOccurs="1" maxOccurs="unbounded"/>   <xs:element name="address"  type="AddressType" minOccurs="1" maxOccurs="1"/>  </xs:sequence>  <xs:attribute name="participantGridId" type="xs:string" /> </xs:complexType><xs:complexType name="IdentifierType">  <xs:sequence>   <xs:element name="source"   type="xs:string"/>   <xs:element name="type" type="xs:string"/>   <xs:element name="value"   type="xs:string"/>   <xs:element name="isprimary"  type="xs:boolean"/>  </xs:sequence> </xs:complexType>  <xs:complexType name="AddressType">	<xs:sequence>		<xs:element name="city" type="xs:string"/>		<xs:element name="countryCode" type="xs:string"/>		<xs:element name="postalCode" type="xs:string"/>		<xs:element name="stateCode" type="xs:string"/>		<xs:element name="streetAddress" type="xs:string"/>	</xs:sequence></xs:complexType><xs:complexType name="StudyType">  <xs:sequence>    <xs:element name="identifier"  type="IdentifierType" minOccurs="1" maxOccurs="unbounded"/>  </xs:sequence></xs:complexType>  </xs:schema>